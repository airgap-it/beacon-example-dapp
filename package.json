{
  "name": "beacon-example-dapp",
  "version": "0.0.1",
  "author": "Papers GmbH <contact@papers.ch> (https://papers.ch)",
  "homepage": "https://walletbeacon.io",
  "scripts": {
    "postinstall": "node scripts/patch-crypto.js && npm run prepare-prod-build",
    "ng": "ng",
    "serve": "ionic serve --external",
    "start": "ng serve",
    "build": "ng build",
    "deploy": "ng deploy",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "lint:fix": "ng lint --fix",
    "prettier": "prettier --write 'src/**/*'",
    "prepare-prod-build": "npm run disable-pure-getters && npm run configure-mangle",
    "disable-pure-getters": "replace 'pure_getters: buildOptions.buildOptimizer' 'pure_getters: false' ./node_modules/@angular-devkit/build-angular/src/angular-cli-files/models/webpack-configs/common.js ",
    "configure-mangle": "replace 'safari10: true' 'safari10: true, keep_fnames: true' ./node_modules/@angular-devkit/build-angular/src/angular-cli-files/models/webpack-configs/common.js "
  },
  "private": true,
  "dependencies": {
    "@airgap/beacon-sdk": "2.0.0-beta.19",
    "@angular/common": "^9.1.2",
    "@angular/core": "^9.1.2",
    "@angular/forms": "^9.1.2",
    "@angular/platform-browser": "^9.1.2",
    "@angular/platform-browser-dynamic": "^9.1.2",
    "@angular/router": "^9.1.2",
    "@ionic/angular": "5.0.7",
    "@ionic/storage": "^2.2.0",
    "@sentry/browser": "^5.15.5",
    "airgap-coin-lib": "0.8.0",
    "core-js": "^2.5.4",
    "rxjs": "~6.5.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.901.2",
    "@angular-devkit/build-angular": "~0.901.2",
    "@angular-devkit/core": "^9.1.2",
    "@angular-devkit/schematics": "^9.1.2",
    "@angular/cli": "^9.1.2",
    "@angular/compiler": "^9.1.2",
    "@angular/compiler-cli": "^9.1.2",
    "@angular/language-service": "^9.1.2",
    "@ionic/angular-toolkit": "^2.2.0",
    "@types/chrome": "0.0.104",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.12.21",
    "angular-cli-ghpages": "^0.6.2",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "prettier": "^2.0.5",
    "protractor": "~5.4.0",
    "replace": "^1.1.1",
    "ts-node": "~8.1.0",
    "tslint": "~5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-valorsoft": "^2.2.1",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "~3.8.3"
  },
  "description": "Connect Wallets with dApps on Tezos"
}
